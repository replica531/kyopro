  "version": "2.0.0",
  "tasks": [
      {
          "label": "build before debug",
          "type": "shell",
          "command": "g++",
          "args": ["-g", "${fileDirname}/${fileBasename}", "-o", "${fileDirname}/a.out"],
          "group": {
              "kind": "build",
              "isDefault": true
          }
      }
  ]
}